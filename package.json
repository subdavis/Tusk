{
  "name": "Tusk",
  "description": "A modern keepass extension",
  "version": "2.0.0",
  "type": "module",
  "author": "Brandon Davis <brandon@subdavis.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clear": "rm -rf extension/dist extension/manifest.json 'extension.*'",
    "dev": "run-s clear && NODE_ENV=development run-p 'dev:*'",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "dev:background": "run-s build:background -- --mode development",
    "dev:js": "run-s build:js -- --mode development",
    "build": "NODE_ENV=production run-s clear build:web build:prepare build:background build:js",
    "build:ff": "NODE_ENV=production TARGET=firefox run-s clear build:web build:prepare build:background build:js",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config vite.config.background.mts",
    "build:js": "vite build --config vite.config.content.mts",
    "build:web": "vite build",
    "format": "eslint --fix --ext .ts,.js,.vue,.html src/ scripts/ services/ background/",
    "pack:zip": "rm -f extension.zip && zip -r extension.zip extension/*"
  },
  "dependencies": {
    "@materializecss/materialize": "^2.2.1",
    "@vue/compat": "^3.5.13",
    "argon2-browser": "^1.18.0",
    "axios": "^1.8.4",
    "base64-arraybuffer": "^1.0.2",
    "case": "^1.5.4",
    "font-awesome": "^4.7.0",
    "json-formatter-js": "^2.2.0",
    "kdbxweb": "^2.1.1",
    "pako": "^1.0.6",
    "vue": "3",
    "vue-simple-spinner": "^1.2.10",
    "webdav": "^5.8.0",
    "webextension-polyfill": "^0.12.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "packageManager": "yarn@4.7.0",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/webextension-polyfill": "^0.10.7",
    "@vitejs/plugin-vue": "^5.2.3",
    "bestzip": "^2.2.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^10.0.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.2.0",
    "globals": "^16.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "vite": "^5.4.14",
    "vite-plugin-wasm": "^3.3.0",
    "vue-eslint-parser": "^10.1.1"
  }
}
