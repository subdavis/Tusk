{
  "name": "Tusk",
  "description": "A modern keepass extension",
  "version": "2.0.0",
  "author": "Brandon Davis <brandon@subdavis.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-tests": "cross-env NODE_ENV=test webpack --config webpack.test.config.js 'mocha-loader!./tests/test.js' --output-filename tests/bundle.build.js",
    "bundle": "./packer.sh chrome && ./packer.sh firefox",
    "clean": "rm -rf dist/ *.zip manifest.json demo*",
    "watch": "gulp watch",
    "watchtests": "gulp watchtests"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "base64-arraybuffer": "^0.1.5",
    "case": "^1.5.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "json-formatter-js": "^2.2.0",
    "kdbxweb": "^1.0.2",
    "materialize-css": "^0.100.2",
    "pako": "^1.0.6",
    "vue": "^2.4.4",
    "vue-simple-spinner": "^1.2.8",
    "xmldom": "^0.1.27"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "mocha": "^4.1.0",
    "mocha-loader": "^1.1.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "should": "^13.2.0",
    "sinon": "^4.1.4",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "vue-loader": "11.1.4",
    "vue-template-compiler": "^2.4.4",
    "wasm-loader": "^1.1.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.1",
    "webpack-stream": "^4.0.0"
  }
}
