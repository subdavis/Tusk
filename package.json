{
  "name": "ckpx",
  "description": "A modern keepass extension",
  "version": "2.0.0",
  "author": "Brandon Davis <brandon@subdavis.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist && rm ckpx.zip",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "watch": "gulp watch",
    
    "package": "zip -r ckpx manifest.json  *.html dist",
    "prepackage": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "base64-arraybuffer": "^0.1.5",
    "case": "^1.5.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "json-formatter": "^0.1.0",
    "json-formatter-js": "^2.2.0",
    "kdbxweb": "^1.0.2",
    "materialize-css": "^0.100.2",
    "pako": "^1.0.6",
    "vue": "^2.4.4",
    "vue-simple-spinner": "^1.2.8",
    "xmldom": "^0.1.27"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-stream": "^4.0.0"
  }
}
